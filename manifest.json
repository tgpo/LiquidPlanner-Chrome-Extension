{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "images/icon.png",
      "default_popup": "popup.html"
   },
   "description": "Get your LiquidPlanner Tasks in Google Chrome",
   "icons": {
      "128": "images/icon-128.png",
      "16": "images/icon.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png"
   },
   "manifest_version": 2,
   "name": "LiquidPlanner Extension for Chrome",
   "options_page": "options.html",
   "permissions": [ "https://*.liquidplanner.com/", "https://*.liquidplannerlab.com/", "notifications" ],
   "version": "2",
   "web_accessible_resources": [ "images/icon-48.png" ]
}
